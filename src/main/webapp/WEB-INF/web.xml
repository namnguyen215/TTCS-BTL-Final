<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
    <filter-name>f1</filter-name>
    <filter-class>controller.filters.MyFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>f1</filter-name>
    <url-pattern>/manage-customer</url-pattern>
  </filter-mapping>

  <error-page>
    <error-code>500</error-code>
    <location>/page/loi/500.html</location>
  </error-page>
  <error-page>
    <error-code>404</error-code>
    <location>/page/loi/404.html</location>
  </error-page>
</web-app>
